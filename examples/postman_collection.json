              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"current_password\": \"SecurePass123!\",\n  \"new_password\": \"NewSecurePass123!\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/users/me/change-password",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "users", "me", "change-password"]
            }
          }
        }
      ]
    },
    {
      "name": "Exercises",
      "item": [
        {
          "name": "List Global Exercises",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const response = pm.response.json();",
                  "    if (response.data && response.data.length > 0) {",
                  "        pm.collectionVariables.set('exercise_id', response.data[0].id);",
                  "    }",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/v1/exercises?page=1&limit=20",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "exercises"],
              "query": [
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "20"
                },
                {
                  "key": "search",
                  "value": "bench",
                  "disabled": true
                },
                {
                  "key": "equipment",
                  "value": "BARBELL",
                  "disabled": true
                },
                {
                  "key": "muscle_group",
                  "value": "CHEST",
                  "disabled": true
                }
              ]
            }
          }
        },
        {
          "name": "Get Exercise by ID",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/v1/exercises/{{exercise_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "exercises", "{{exercise_id}}"]
            }
          }
        },
        {
          "name": "Create Custom Exercise",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Cable Fly Variation\",\n  \"description\": \"Modified cable fly for upper chest\",\n  \"equipment\": \"CABLE\",\n  \"muscle_contributions\": {\n    \"CHEST\": 1.0,\n    \"FRONT_DELTS\": 0.25\n  },\n  \"difficulty\": \"INTERMEDIATE\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/exercises",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "exercises"]
            }
          }
        },
        {
          "name": "Search Exercises",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"exercises for building bigger arms\",\n  \"limit\": 10\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/exercises/search",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "exercises", "search"]
            }
          }
        },
        {
          "name": "Get Equipment Types",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/v1/exercises/equipment-types",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "exercises", "equipment-types"]
            }
          }
        },
        {
          "name": "Get Muscle Groups",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/v1/exercises/muscle-groups",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "exercises", "muscle-groups"]
            }
          }
        }
      ]
    },
